{
    "extends": "next/core-web-vitals",
    "env": {
        "es6": true
    },
    "rules": {
        "semi": "error",
        "prefer-const": "warn",
        "consistent-return": "warn",
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": "error",
        "camelcase": "warn",
        "quotes": "error"
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    }
}